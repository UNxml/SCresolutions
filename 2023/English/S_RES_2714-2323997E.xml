      <p xml:space="preserve">
        <docTitle refersTo="#resolutionNumber">		Resolution 2714 (2023)</docTitle>
      </p>
      <p xml:space="preserve">
        <docTitle refersTo="#adoption">Adopted by the Security Council at its <inline name="adoptionMeeting" title="9491st">9491st meeting</inline>, on<docDate date="2023-12-01" refersTo="#adoptionDate">1 December 2023</docDate></docTitle>
