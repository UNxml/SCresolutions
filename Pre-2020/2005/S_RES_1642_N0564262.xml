      <p xml:space="preserve">
        <docTitle refersTo="#resolutionNumber">		Resolution 1642 (2005)</docTitle>
      </p>
      <p xml:space="preserve">
        <docTitle refersTo="#adoption">Adopted by the Security Council at its <inline name="adoptionMeeting" title="5324th">5324th meeting</inline>, on<docDate date="2005-12-14" refersTo="#adoptionDate">14 December 2005</docDate></docTitle>
