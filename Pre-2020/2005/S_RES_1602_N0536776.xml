      <p xml:space="preserve">
        <docTitle refersTo="#resolutionNumber">		Resolution 1602 (2005)</docTitle>
      </p>
      <p xml:space="preserve">
        <docTitle refersTo="#adoption">Adopted by the Security Council at its <inline name="adoptionMeeting" title="5193rd">5193rd meeting</inline>, on<docDate date="2005-05-31" refersTo="#adoptionDate">31 May 2005</docDate></docTitle>
