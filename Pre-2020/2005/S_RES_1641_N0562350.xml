    <mainBody>
      <paragraph eId="para_6" class="SingleTxt">
        <num>1.</num>
        <content eId="para_6__content">
          <p xml:space="preserve">	<span class="italic">Decides</span> to extend the mandate of the United Nations Operation in Burundi (ONUB) until 15 January 2006;</p>
        </content>
      </paragraph>
      <paragraph eId="para_7" class="SingleTxt">
        <num>2.</num>
        <content eId="para_7__content">
          <p xml:space="preserve">	<span class="italic">Decides</span> to remain actively seized of the matter.</p>
        </content>
      </paragraph>
