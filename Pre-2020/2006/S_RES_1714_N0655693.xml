      <p xml:space="preserve">
        <docTitle refersTo="#resolutionNumber">		Resolution 1714 (2006)</docTitle>
      </p>
      <p xml:space="preserve">
        <docTitle refersTo="#adoption">Adopted by the Security Council at its <inline name="adoptionMeeting" title="5545th">5545th meeting</inline>, on<docDate date="2006-10-06" refersTo="#adoptionDate">6 October 2006</docDate></docTitle>
