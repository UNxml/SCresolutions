      <p xml:space="preserve">
        <docTitle refersTo="#resolutionNumber">		Resolution 1689 (2006)</docTitle>
      </p>
      <p xml:space="preserve">
        <docTitle refersTo="#adoption">Adopted by the Security Council at its <inline name="adoptionMeeting" title="5468th">5468th meeting</inline>, on<docDate date="2006-06-20" refersTo="#adoptionDate">20 June 2006</docDate></docTitle>
