      <p xml:space="preserve">
        <docTitle refersTo="#adoption">Adopted by the Security Council at its <inline name="adoptionMeeting" title="6450th">6450th meeting</inline>, on<docDate date="2010-12-15" refersTo="#adoptionDate">15 December 2010</docDate></docTitle>
