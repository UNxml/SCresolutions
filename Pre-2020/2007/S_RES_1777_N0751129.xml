      <p xml:space="preserve">
        <docTitle refersTo="#resolutionNumber">		Resolution 1777 (2007)</docTitle>
      </p>
      <p xml:space="preserve">
        <docTitle refersTo="#adoption">Adopted by the Security Council at its <inline name="adoptionMeeting" title="5745th">5745th meeting</inline> on<docDate date="2007-09-20" refersTo="#adoptionDate">20 September 2007</docDate></docTitle>
