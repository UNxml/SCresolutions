      <p xml:space="preserve">
        <docTitle refersTo="#adoption">Adopted by the Security Council at its <inline name="adoptionMeeting" title="5699th">5699th meeting</inline>, on<docDate date="2007-06-20" refersTo="#adoptionDate">20 June 2007</docDate></docTitle>
