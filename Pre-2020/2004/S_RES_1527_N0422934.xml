      <p xml:space="preserve">
        <docTitle refersTo="#resolutionNumber">		Resolution 1527 (2004)</docTitle>
      </p>
      <p xml:space="preserve">
        <docTitle refersTo="#adoption">Adopted by the Security Council at its <inline name="adoptionMeeting" title="4909th">4909th meeting</inline>, on<docDate date="2004-02-04" refersTo="#adoptionDate">4 February 2004</docDate></docTitle>
