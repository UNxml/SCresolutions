      <p xml:space="preserve">
        <docTitle refersTo="#resolutionNumber">		Resolution 1838 (2008)</docTitle>
      </p>
      <p xml:space="preserve">
        <docTitle refersTo="#adoption">Adopted by the Security Council at its <inline name="adoptionMeeting" title="5987th">5987th meeting</inline>, on<docDate date="2008-10-07" refersTo="#adoptionDate">7 October 2008</docDate></docTitle>
