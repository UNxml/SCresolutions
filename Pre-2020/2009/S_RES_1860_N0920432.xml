      <p xml:space="preserve">
        <docTitle refersTo="#resolutionNumber">		Resolution 1860 (2009)</docTitle>
      </p>
      <p xml:space="preserve">
        <docTitle refersTo="#adoption">Adopted by the Security Council at its <inline name="adoptionMeeting" title="6063rd">6063rd meeting</inline>, on<docDate date="2009-01-08" refersTo="#adoptionDate">8 January 2009</docDate></docTitle>
