    <mainBody>
      <paragraph eId="para_7" class="SingleTxt">
        <num>1.</num>
        <content eId="para_7__content">
          <p xml:space="preserve">	<span class="italic">Decides</span> to extend the authorizations as set out in resolution 2420 for a further 12 months from the date of this resolution; </p>
        </content>
      </paragraph>
      <paragraph eId="para_8" class="SingleTxt">
        <num>2.</num>
        <content eId="para_8__content">
          <p xml:space="preserve">	<span class="italic">Requests</span> the Secretary-General to report to the Security Council within eleven months of the adoption of this resolution on its implementation; </p>
        </content>
      </paragraph>
      <paragraph eId="para_10" class="SingleTxt">
        <num>3.</num>
        <content eId="para_10__content">
          <p xml:space="preserve">	<span class="italic">Decides</span> to remain actively seized of the matter</p>
        </content>
