                <FRBRthis value="/akn/un/statement/deliberation/unsc/2001-11-12/1377-2001/eng@!main/annex"></FRBRthis>
                <FRBRuri value="/akn/un/statement/deliberation/unsc/2001-11-12/1377-2001/eng@/"></FRBRuri>
                <FRBRdate date="2001-11-12" name="publication"></FRBRdate>
                <FRBRauthor href="sc" as="issuer"></FRBRauthor>
