    <mainBody>
      <paragraph eId="para_2" class="SingleTxt">
        <content eId="para_2__content">
          <p xml:space="preserve">	Decides to adopt the attached declaration on the global effort to combat terrorism.</p>
        </content>
      </paragraph>
    </mainBody>
    <attachments>
      <attachment>
        <doc name="annex">
          <meta>
            <identification source="#DGACM">
              <FRBRWork>
