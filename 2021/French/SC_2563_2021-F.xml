    <preamble>
      <formula name="enactingFormula" eId="formula_1">
        <p xml:space="preserve">
          <span class="italic">Le Conseil de sécurité</span>,</p>
      </formula>
      <container name="Rappelant" eId="container_2">
        <p xml:space="preserve">
          <span class="italic">	Rappelant</span> toutes ses résolutions antérieures et toutes les déclarations de sa présidence concernant la situation en Somalie et <span class="italic">soulignant </span>qu’il importe de s’y conformer pleinement,</p>
      </container>
      <container name="Considérant" eId="container_3">
        <p xml:space="preserve">	<span class="italic">Considérant</span> que la situation en Somalie continue de menacer la paix et la sécurité internationales,</p>
      </container>
      <container name="Agissant" eId="container_4">
        <p xml:space="preserve">	<span class="italic">Agissant</span> en vertu du Chapitre VII de la Charte des Nations Unies,</p>
      </container>
